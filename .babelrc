{
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-decorators"
  ],
  "presets": [["env", { "modules": false }], "stage-0", "react"],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
