{
  "name": "taplauncher",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "deploy": "npm run client:build && aws s3 sync build/ s3://esprezzo.io --profile esprezzo",
    "start": "node index.js",
    "client:dev": "webpack-dashboard -t 'TheChain' -- webpack-dev-server",
    "client:watch": "webpack",
    "client:build": "npm run clean && cross-env NODE_ENV=\"production\" webpack -p --progress",
    "client:preview": "npm run clean && cross-env NODE_ENV=\"production\" webpack-dev-server",
    "client:build:heroku": "npm run clean && webpack",
    "clean": "rm -rf ./build",
    "postinstall": "npm run client:build:heroku",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "apollo-client": "^2.0.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "graphql-tag": "^2.5.0",
    "html-webpack-plugin": "^2.24.1",
    "immutable": "^3.8.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-svg-loader": "^1.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1",
    "webpack": "^3.4.1",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.6.1"
  },
  "description": "Frontend boilerplate and webpack build for React/Redux/GraphQL projects",
  "author": "MarcusHurney"
}
